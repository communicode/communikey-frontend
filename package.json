{
  "dependencies": {
    "antd": "2.11.0",
    "axios": "0.15.3",
    "babel-core": "6.22.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.1",
    "babel-loader": "6.2.10",
